{
	"name": "Market Alerts",
	"description" : "Gives alerts for markets",
	"version": "1.0",
	"manifest_version": 2,
	"browser_action": {
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["background-script.js"],
		"persistent": false
	},
	"permissions": [
		"activeTab",
		"storage",
		"*://*/*"
	]
}
